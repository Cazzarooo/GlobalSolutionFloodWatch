@model UsuarioModel

<div class="row">
    <form asp-action="Create">
        <div class="form-goup">
            <label asp-for="Nome"></label>
            <input asp-for="Nome" />
        </div>
        <div class="form-goup">
            <label asp-for="DataNascimento"></label>
            <input asp-for="DataNascimento" />
        </div>
        <div class="form-goup">
            <label asp-for="Localizacao"></label>
            <input asp-for="Localizacao" />
        </div>
        <div>
            <input type="submit" value="Salvar" />
        </div>
    </form>
</div>
